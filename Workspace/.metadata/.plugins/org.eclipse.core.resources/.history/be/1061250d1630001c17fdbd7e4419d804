package tngod.tngoddhekq;

public class Q5_2오답 {
	public static void main(String[] args) {
		int[][] arr = new int[][] {
			{2,3,4,5,6},
			{4,6,8,10,12},
			{6,9,12,15,18},
			{8,12,16,20,24}
		};
		int[][] temp = new int[][] {{0,0,0,0,0}};
		for (int i = 0; i < arr.length; i++) {
			temp[1][5] = arr[1][i];
			arr[1][i] = arr[5][i];
			arr[5][i]= temp[1][5];
		}
		
		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; j < arr[i].length; j++) {
				System.out.println(arr[i][j]);
			}
		}
	}
}
