package string_array.array;

import java.util.Scanner;

public class Imgaram4 {
	public static void main(String[] args) {
		int[] numArr = new int[10];
		int[] counter = new int[10];
		Scanner sc = new Scanner(System.in);
		for (int i = 0; i < numArr.length; i++) {
			numArr[i]=sc.nextInt();
			switch (numArr[i]) {
			case 0:
				counter[0]++;
				break;
			case 1:
				counter[1]++;
				break;
			case 2:
				counter[2]++;
				break;
			case 3:
				counter[3]++;
				break;
			case 4:
				counter[4]++;
				break;
			case 5:
				counter[5]++;
				break;
			case 6:
				counter[6]++;
				break;
			case 7:
				counter[7]++;
				break;
			case 8:
				counter[8]++;
				break;
			case 9:
				counter[9]++;
				break;
			default:
				break;
			}
		}
		
		System.out.println();
		for (int i = 0; i < counter.length; i++) {
			System.out.println(i+"의 개수 : "+counter[i]);
		}
	}
}
