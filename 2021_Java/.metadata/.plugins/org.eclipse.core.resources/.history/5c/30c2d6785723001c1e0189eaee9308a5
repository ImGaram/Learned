package conquest;

public class Q10 {
	public static void main(String[] args) {
		int sum = 0;	// total
		int s = 1;		// 부호 바꾸기
		int num = 0;
		
		while (sum<=100) {
			num++;	// 1
			sum+=num;
			if (num%2==0) {	//-2
				num++;
				num = num * -s;
				//sum+=num;
				
			}
			System.out.println(num);
		}
	}
}
