package conquest;

public class Q8 {
	public static void main(String[] args) {
		int temp = 0;
		int x=1, y=2, z=3;
		System.out.println("x = "+x+", y = "+y+", z = "+z);
		
		temp = x;
		x =y;
		y = z;
		z = temp;
		
		System.out.println("변경 후");
		System.out.println("x = "+x+", y = "+y+", z = "+z);
	}
}
