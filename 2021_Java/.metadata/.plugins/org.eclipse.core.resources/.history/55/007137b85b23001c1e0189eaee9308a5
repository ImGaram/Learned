package conquest;

public class Q9 {
	public static void main(String[] args) {
		int[][] arr = {
				{5,5,5,5,5},
				{10,10,10,10,10},
				{20,20,20,20,20},
				{30,30,30,30,30}
		};
		int total = 0;
		double avg;
		
		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; j < arr[i].length; j++) {
				total+=arr[i][j];
			}
		}
		System.out.println("total = "+total);
		avg = total/20.0;
		System.out.println("average = "+avg);
	}
}
