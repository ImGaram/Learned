package tngod.tngoddhekq;

public class Q5_2오답 {
	public static void main(String[] args) {
		int[][] arr = new int[][] {
			{2,3,4,5,6},
			{4,6,8,10,12},
			{6,9,12,15,18},
			{8,12,16,20,24}
		};
		for (int i = 0; i < arr.length; i++) {
			int temp;
			temp = arr[i][4];
			arr[i][4] = arr[i][0];
			arr[i][0] = temp; 
		}
		
		for (int[] is : arr) {
			for (int js : is) {
				System.out.print(js+" ");
			}
			System.out.println();
		}
	}
}
