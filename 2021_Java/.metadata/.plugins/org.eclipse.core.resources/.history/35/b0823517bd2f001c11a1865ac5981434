package inheritance;

import java.util.Scanner;
import inheritance.pack.Pack1;
import inheritance.pack.subpack.Pack2;
// 방법 (1) = import inheritance.pack.Pack1;

public class Inheritance4 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		System.out.println("출력 : "+a);
		
		// 방법 (2) = inheritance.pack.Pack1 pp = new inheritance.pack.Pack1();
		Pack1 pp = new Pack1();
		pp.x = a;
		pp.print();
		
		Pack2 pp2 = new Pack2();
		pp2.subPrint();
	}
}
